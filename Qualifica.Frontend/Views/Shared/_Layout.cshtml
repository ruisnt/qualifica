<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>Qualifica</title>
    <meta name="description" content="Qualifica">
    <meta name="author" content="Grupo Qualifica">

    <!-- Favicon -->
    <link rel="shortcut icon" href="@Url.Content("~/assets/images/favicon.ico")">

    <!-- Bootstrap CSS -->
    <link href="@Url.Content("~/assets/css/bootstrap.min.css")" rel="stylesheet" type="text/css" />

    <!-- Font Awesome CSS -->
    <link href="@Url.Content("~/assets/font-awesome/css/font-awesome.min.css")" rel="stylesheet" type="text/css" />

    <!-- Custom CSS -->
